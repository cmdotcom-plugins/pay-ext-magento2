parameters:
    level: 1
        reportUnmatchedIgnoredErrors: false
        ignoreErrors:
        - '#Variable \$block might not be defined.#'
        - '#Undefined variable: \$block#'
        - '#Variable \$escaper might not be defined.#'
        - '#Undefined variable: \$escaper#'
    fileExtensions:
        - php
        - phtml
    excludes_analyse:
        - Test/*
